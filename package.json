{
  "name": "mcp-server",
  "module": "src/index.ts",
  "type": "module",
  "version": "1.0.0",
  "description": "Model Context Protocol (MCP) Server",
  "private": true,
  "scripts": {
    "inspect": "npm run build && npx @modelcontextprotocol/inspector node -- build/index.js",
    "start": "npx bun run src/index.ts",
    "build": "npx bun build src/index.ts --outdir build --target node",
    "build:http": "npx bun build src/server/http-server.ts --outdir build --target node",
    "dev": "npx bun --watch src/index.ts",
    "start:http": "npx bun run src/server/http-server.ts",
    "dev:http": "npx bun --watch src/server/http-server.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.11.0"
  },
  "peerDependencies": {
    "@valibot/to-json-schema": "^1.0.0",
    "effect": "^3.14.4",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "fastmcp": "^1.21.0",
    "marked": "^15.0.12",
    "open": "^10.1.2",
    "vue-component-meta": "^2.2.10",
    "zod": "^3.24.2"
  }
}